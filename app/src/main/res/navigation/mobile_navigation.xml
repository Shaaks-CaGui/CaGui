<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.sparklead.cagui.ui.fragments.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_explore"
        android:name="com.sparklead.cagui.ui.fragments.ExploreFragment"
        android:label="@string/title_explore"
        tools:layout="@layout/fragment_explore" />

    <fragment
        android:id="@+id/navigation_admissions"
        android:name="com.sparklead.cagui.ui.fragments.AdmissionFragment"
        android:label="@string/title_admissions"
        tools:layout="@layout/fragment_admission" />

    <fragment
        android:id="@+id/navigation_faqs"
        android:name="com.sparklead.cagui.ui.fragments.AdmissionFragment"
        android:label="@string/title_faqs"
        tools:layout="@layout/fragment_faqs" />
</navigation>